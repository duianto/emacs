(when (not (locate-library "apel-ver"))
      (add-to-list 'load-path "../apel"))
(when (not (locate-library "mime"))
      (add-to-list 'load-path "../flim"))
(when (not (locate-library "mime-view"))
      (add-to-list 'load-path "../semi"))
(when (not (locate-library "wl"))
      (add-to-list 'load-path "../wl/wl")
      (add-to-list 'load-path "../wl/elmo")
      (add-to-list 'load-path "../wl/utils"))
